package ca.ubc.cs.cpsc210.quiz.model;

import java.util.List;

/**
 * Created by Jonathan Stiansen on 2014-09-05.
 *
 * Represents a geographical area.
 */
public interface GeoArea {

    /**
     * Gets string that describes this geographical area
     *
     * @return string that describes geographical area
     */
    String getCityString();

    /**
     * Returns geographic strings that could be used to describe this area.
     *
     * @return array of strings that could be used to describe the area
     */
    List<String> getAllGeoStrings();
}
